{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"]
    },
    "plugins": [
        "react"
    ],
    "extends": "airbnb"
}